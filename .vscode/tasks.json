{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/data_structure/week5/main.c",
                "${workspaceFolder}/data_structure/week5/stack.c",
                "${workspaceFolder}/data_structure/week5/elem.c",
                "-o",
                "${workspaceFolder}/data_structure/week5/main"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
} 